//   FUNCTION Definition: xf_QuadQuadrilateral
int 
xf_QuadQuadrilateral( int Order, int *pnq, real **pxq, real **pwq)
{
  int ierr, nq1;
  int ix, iy, ny, iq;
  real *xq1, *wq1;

  xq1 = NULL;
  wq1 = NULL;
  
  // obtain 1D quad points and weights
  ierr = xf_Error(xf_QuadLine(Order, &nq1, &xq1, &wq1));
  if (ierr != xf_OK) return ierr;

  // form tensor product of points and weights
  (*pnq) = nq1*nq1;
  ierr = xf_Error(xf_ReAlloc( (void **) pxq, (*pnq)*2, sizeof(real)));
  if (ierr != xf_OK) return ierr;
  ierr = xf_Error(xf_ReAlloc( (void **) pwq, (*pnq), sizeof(real)));
  if (ierr != xf_OK) return ierr;

  for (iy=0; iy<nq1; iy++)
    for (ix=0, ny=nq1*iy; ix<nq1; ix++){
      iq = ny+ix;
      (*pxq)[2*iq+0] = xq1[ix];
      (*pxq)[2*iq+1] = xq1[iy];
      (*pwq)[iq] = wq1[ix]*wq1[iy];
    }

  // release 1d points + weights
  xf_Release( (void *) xq1);
  xf_Release( (void *) wq1);
  
  return xf_OK;
}


